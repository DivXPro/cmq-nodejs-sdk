    "@types/node": "^10.12.21",
    "@types/supertest": "^2.0.7",
    "jest": "^24.0.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.4",
    "supertest": "^3.4.2",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "tsconfig-paths": "^3.7.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.1"
